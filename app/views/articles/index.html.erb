<p style="color: green"><%= notice %></p>
 <%= stylesheet_link_tag "art" %>
<h1>Articles</h1>

<div id="articles">
  <% @articles.each do |article| %>
    <%= render article %>
     <div class="articles">
        <h2 class="articles-title">
            <h3>
                <%= article.title %>
            </h3>
        </h2>
      </div>
    <p>
      <%= link_to "Show this article", article %>
    </p>
  <% end %>
</div>
<br>
<%= link_to "New article", new_article_path %>
<%= link_to "Go back", root_path %>

